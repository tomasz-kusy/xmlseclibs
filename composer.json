  "description": "A PHP library for XML Security",
  "license": "BSD-3-Clause",
  "keywords": [
    "xml",
    "xmldsig",
    "signature",
    "security"
  ],
  "homepage": "https://github.com/tomasz-kusy/xmlseclibs",
  "autoload": {
    "psr-4": {
      "RobRichards\\XMLSecLibs\\": "src"
    }
  },
  "require": {
    "php": ">= 5.4",
    "ext-openssl": "*",
    "ext-bcmath": "*"
  },
  "replace": {
    "robrichards/xmlseclibs": "3.1.0"
  }
}
